<%= render 'layouts/header', {header: "Sign Up", subheader: "Only a few text fields away from joining the greatest social networking site ever.", feed: false} %>
<div class="container">
  <div class="row">
    <div class="span4">
      <%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :class => "well"}) do |f| %>
        <%= devise_error_messages! %>

        <h1>Sign up here</h1>

        <%= f.input :first_name, :autofocus => true %>
        <%= f.input :last_name %>
        <%= f.input :profile_name %>

        <%= f.input :email %>
        <%= f.input :password %>
        <%= f.input :password_confirmation %>


        <%= f.submit "Sign up", class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>
<%= render "devise/shared/links" %>