<div class="main-header">
  <h1><%= header %></h1>
  <p><%= subheader %></p>
  <% if feed %>
	  <%= link_to "Post a New Status", new_status_path, class: "btn btn-warning btn-large" %>
  <% end %>
</div>
<div class="container">
<% flash.each do |type, message| %>
    <div class="alert <%= flash_class type %>">
      <button class="close" data-dismiss="alert">x</button>
      <%= message %>
    </div>
<% end %>
</div>